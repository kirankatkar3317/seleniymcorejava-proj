
public class AustraianSignal implements WorldsSignal, Cars {
	
	//implements keyword implement interface

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	//interface class of object for interface method fetch
		
		WorldsSignal a = new AustraianSignal();
		a.red();
		a.green();
		a.yellow();
		
	//	local class of object for local method fetch
		AustraianSignal b = new AustraianSignal();
		b.kk();
		
		
	}

	@Override
	public void green() {
		// TODO Auto-generated method stub
		System.out.println("signal green");
	}

	@Override
	public void red() {
		// TODO Auto-generated method stub
		System.out.println("signal red");
	}

	@Override
	public void yellow() {
		// TODO Auto-generated method stub
		System.out.println("signal yellow");
	}
	
	public void kk() {
		System.out.println("kk1234");
	}

	@Override
	public void sportscar() {
		// TODO Auto-generated method stub
		System.out.println("its a sports car");
	}

	@Override
	public void normalcar() {
		// TODO Auto-generated method stub
		System.out.println("its a normal car");
	}

}
